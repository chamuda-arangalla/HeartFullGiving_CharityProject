<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Charity_RegisterationForm">

    <Button
        android:id="@+id/button"
        android:layout_width="62dp"
        android:layout_height="39dp"
        android:backgroundTint="#E9525353"
        android:freezesText="false"
        android:text="back"
        android:textColor="#FFFFFF"
        android:textSize="8sp"
        android:textStyle="normal"
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/edtEmail2"
        android:layout_width="186dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="enter official email"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="187dp"
        tools:layout_editor_absoluteY="275dp" />

    <TextView
        android:id="@+id/labeltxtEmail2"
        android:layout_width="185dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Email :"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/edtEmail"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="290dp" />

    <TextView
        android:id="@+id/titleCharityRegister"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Charity Registration"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="48dp" />

    <EditText
        android:id="@+id/edtCharityname"
        android:layout_width="186dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="enter charity name"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="187dp"
        tools:layout_editor_absoluteY="118dp" />

    <TextView
        android:id="@+id/labeltxtName"
        android:layout_width="185dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Name :"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/edtCharityname"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="130dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="198dp"
        android:layout_height="137dp"
        app:srcCompat="@drawable/fotor_2023_3_17_10_12_34"
        tools:layout_editor_absoluteX="312dp"
        tools:layout_editor_absoluteY="604dp" />

    <EditText
        android:id="@+id/edtOgname"
        android:layout_width="186dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="enter organizer name"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="187dp"
        tools:layout_editor_absoluteY="171dp" />

    <TextView
        android:id="@+id/labeltxtOrganizer"
        android:layout_width="185dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Organizer name :"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/edtOgname"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="183dp" />

    <EditText
        android:id="@+id/edtwebsite"
        android:layout_width="186dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="enter registration id"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="188dp"
        tools:layout_editor_absoluteY="223dp" />

    <TextView
        android:id="@+id/labeltxtOffweb"
        android:layout_width="168dp"
        android:layout_height="37dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Voluntary social services id:"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/edtwebsite"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="225dp" />

    <EditText
        android:id="@+id/edtPassword"
        android:layout_width="186dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="enter password"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="187dp"
        tools:layout_editor_absoluteY="535dp" />

    <TextView
        android:id="@+id/labeltxtPassword"
        android:layout_width="185dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Password :"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/edtPassword"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="548dp" />

    <EditText
        android:id="@+id/edtRetypePassword"
        android:layout_width="186dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="re-enter password"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="187dp"
        tools:layout_editor_absoluteY="589dp" />

    <TextView
        android:id="@+id/labeltxtRetypePassword"
        android:layout_width="166dp"
        android:layout_height="22dp"
        android:layout_marginStart="20dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Retype Password :"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/edtRetypePassword"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="598dp" />

    <Button
        android:id="@+id/btnRegister"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#0CC0DF"
        android:text="Register"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="644dp" />

    <EditText
        android:id="@+id/edtEmail"
        android:layout_width="186dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="enter address"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="188dp"
        tools:layout_editor_absoluteY="334dp" />

    <TextView
        android:id="@+id/labeltxtEmail"
        android:layout_width="168dp"
        android:layout_height="39dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="sans-serif-condensed"
        android:hint="enter address"
        android:text="Charity  registered address :"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/edtEmail"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="337dp" />

    <EditText
        android:id="@+id/edtPersonnumber"
        android:layout_width="186dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="077-xxxxxxx"
        android:inputType="phone"
        tools:layout_editor_absoluteX="188dp"
        tools:layout_editor_absoluteY="430dp" />

    <TextView
        android:id="@+id/labeltxtKeycontact"
        android:layout_width="185dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Key Contact person :"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/edtPersonnumber"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="440dp" />

    <EditText
        android:id="@+id/edtTeleNo"
        android:layout_width="186dp"
        android:layout_height="43dp"
        android:ems="10"
        android:hint="011-xxxxxxx"
        android:inputType="phone"
        tools:layout_editor_absoluteX="188dp"
        tools:layout_editor_absoluteY="387dp" />

    <TextView
        android:id="@+id/labeltxtTeleno"
        android:layout_width="185dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="40dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Telephone No :"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/edtTeleNo"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="395dp" />

    <EditText
        android:id="@+id/edtwebsite2"
        android:layout_width="186dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="enter official website"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="188dp"
        tools:layout_editor_absoluteY="482dp" />

    <TextView
        android:id="@+id/labeltxtOffweb2"
        android:layout_width="185dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Website :"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/edtwebsite"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="496dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="198dp"
        android:layout_height="137dp"
        app:srcCompat="@drawable/fotor_2023_3_17_10_12_34"
        tools:layout_editor_absoluteX="-71dp"
        tools:layout_editor_absoluteY="-102dp" />


</androidx.constraintlayout.widget.ConstraintLayout>